<div class="coaching" id="coaching">
  <a name="coaching"></a>
  <div class="coaching__wrapper">
    <div class="coaching__text">
      <h2>{{coaching.title}}</h2>
      <blockquote>{{coaching.quote}}</blockquote>
      {% for paragraph in coaching.text %}
        <p>{{paragraph}}</p>
      {% endfor %}
      {% if coaching.list %}
      <ul>
        {% for item in coaching.list %}
        <li>{{item}}</li>
        {% endfor %}
      </ul>
      {% endif %}
    </div>
  </div>
</div>